<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist %>
  <%= f.text_field :artist %>
  <%= f.collection_select :genre_name, Genre.all, :name, :name %>
    <input name="song[note_ids][]">
    <input name="song[note_ids][]">
    <input name="song[note_ids][]">
    <input type="submit" value="Submit">

  <%= f.submit %>
<% end %>


